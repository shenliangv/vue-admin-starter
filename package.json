{
  "name": "vue-admin-starter",
  "version": "0.1.0",
  "scripts": {
    "dev": "vue-cli-service serve --mode local",
    "build:dev": "vue-cli-service build --mode dev",
    "build:staging": "vue-cli-service build --mode staging",
    "build:prod": "vue-cli-service build --mode prod",
    "build:report": "vue-cli-service build --mode prod --report",
    "deploy:github": "npm run build:staging && push-dir --dir=dist --branch=gh-pages --cleanup",
    "test:unit": "vue-cli-service test:unit",
    "test:e2e": "vue-cli-service test:e2e",
    "lint": "vue-cli-service lint",
    "commit": "git-cz"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{js,vue,ts}": [
      "vue-cli-service lint",
      "git add"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "dependencies": {
    "axios": "^0.19.0",
    "core-js": "^3.3.2",
    "crypto-js": "^3.1.9-1",
    "element-ui": "^2.12.0",
    "lodash": "^4.17.15",
    "nprogress": "^0.2.0",
    "qs": "^6.9.7",
    "vue": "^2.6.11",
    "vue-router": "^3.0.2",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@types/crypto-js": "^3.1.43",
    "@types/jest": "^24.0.11",
    "@types/lodash": "^4.14.145",
    "@types/nprogress": "^0.2.0",
    "@types/qs": "^6.9.0",
    "@types/tinymce": "^4.5.23",
    "@vue/cli-plugin-babel": "^4.0.0",
    "@vue/cli-plugin-e2e-cypress": "^4.0.0",
    "@vue/cli-plugin-eslint": "^4.0.0",
    "@vue/cli-plugin-router": "^4.0.0",
    "@vue/cli-plugin-typescript": "^4.0.0",
    "@vue/cli-plugin-unit-jest": "^4.0.0",
    "@vue/cli-plugin-vuex": "^4.0.0",
    "@vue/cli-service": "^4.0.0",
    "@vue/eslint-config-prettier": "^5.0.0",
    "@vue/eslint-config-typescript": "^4.0.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "commitizen": "^4.0.3",
    "cz-conventional-changelog": "^3.0.2",
    "eslint": "^5.16.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-vue": "^5.0.0",
    "husky": "^3.0.9",
    "lint-staged": "^9.4.2",
    "prettier": "^1.18.2",
    "push-dir": "^0.4.1",
    "sass": "^1.19.0",
    "sass-loader": "^8.0.0",
    "typescript": "~3.5.3",
    "vue-template-compiler": "^2.6.11"
  }
}
